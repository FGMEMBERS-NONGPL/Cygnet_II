<?xml version="1.0"?>

<PropertyList>
 <path>CanopyControl.ac</path>
 <animation>
  <type>pick</type>
  <object-name>openclose</object-name>
  <object-name>handle</object-name>
  <visible>true</visible>
  <action>
    <name>open-close canopy</name>
    <button>0</button>
    <binding>
      <command>property-toggle</command>
      <property>controls/flight/speedbrake</property>
    </binding>
  </action>
 </animation>
</PropertyList>


